@model NøstedProsjekt.Models.ServiceOrder
<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />


<h1>Registrering i arbeidsdokument</h1>

<form asp-action="CreateServiceOrder" method="post">
     <table class="table table-bordered">
        <thead>
             <tr>
               <th>BOOKET SERVICE TIL UKE:</th>
                <th>Henvendelse mottatt og reg. ddmmåå</th>
                <th>Case ferdig? JA/NEI</th>
                <th>Produkt-type</th>
                <th>Bestilling/feilbeskrivelse fra kunde</th>
               <th>Kundeinfo</th>
               <th>Avtalt levering ddmmåå</th>
               <th>Produkt mottatt ddmmåå</th>
              <th>Avtalt ferdigstillelse innen ddmmåå</th>
               <th>Service ferdig ddmmåå</th>
                <th>Antall timer på utført service</th>
                <th>Ordrenr? JA/NEI</th>
               <th>Service skjema JA/NEI</th>
            </tr> 
        </thead>
        <tbody>
            <tr>
            <td><input asp-for="Week" class="form-control" /></td>
            <td><input asp-for="Inquiry"class="form-controll" /></td>
            <td><select asp-for="CaseCompleted" />
                    <option value="" selected disabled></option>
                    <option value="Ja">Ja</option>
                    <option value="Nei">Nei</option>
                </td>
             <td><input asp-for="ProductType" class="form-control" /></td>
             <td><input asp-for="Comment" class="form-control" /></td>
                <td><input asp-for="CustomerInfo" class="form-controll" /></td>
             <td><input asp-for="PlannedDelivery" class="form-controlå" /></td>
             <td><input type="date" asp-for="ProductReceivedDate" class="form-control" /></td>
            <td><input type="date" asp-for="AgreedCompletionDate" class="form-control" /></td>
             <td><input type="date" asp-for="ServiceCompletedDate" class="form-control" /></td>
            <td><input asp-for="ServiceHours" class="form-control" /></td>
            <td><select asp-for="HasOrderNumber" />
               <option value="" selected disabled></option>
               <option value="Ja">Ja</option>
                <option value="Nei">Nei</option>
                </td>
            <td><select asp-for="HasServiceForm" />
                    <option value="" selected disabled></option>
                    <option value="Ja">Ja</option>
                    <option value="Nei">Nei</option>
                </td>
            </tr>
        </tbody>
    </table>

    <button type="submit" class="btn btn-primary">Opprett</button>
</form>


